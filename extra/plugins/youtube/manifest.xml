<?xml version="1.0" encoding="UTF-8"?>
<vs-manifest type="plugin">
	<metadata>
		<label>p_youtube_plglabel</label>
		<description>p_youtube_plgdesc</description>
		<version>0.2.1</version>
		<status>stable</status>
		<key>youtube</key>
		<file>X/VlcShares/Plugins/Youtube.php</file>
		<class>X_VlcShares_Plugins_Youtube</class>
		<compatibility>
			<from>0.5.4</from>
			<to>0.6</to>
		</compatibility>
	</metadata>
	<files>
		<application>
			<models>
				<file>YoutubeAccount.php</file>
				<file>YoutubeAccountsMapper.php</file>
				<file>YoutubeVideo.php</file>
				<file>YoutubeVideosMapper.php</file>
				<file>YoutubeCategory.php</file>
				<file>YoutubeCategoriesMapper.php</file>
				<DbTable>
					<file>YoutubeAccounts.php</file>
					<file>YoutubeCategories.php</file>
					<file>YoutubeVideos.php</file>
				</DbTable>
			</models>
			<controllers>
				<file>YoutubeController.php</file>
			</controllers>
			<forms>
				<file>YoutubeAccount.php</file>
				<file>YoutubeCategory.php</file>
				<file>YoutubeVideo.php</file>
			</forms>
			<views>
				<scripts>
					<youtube>
						<file>account.phtml</file>
						<file>category.phtml</file>
						<file>video.phtml</file>
						<file>index.phtml</file>
					</youtube>
				</scripts>
			</views>
		</application>
		<library>
			<X>
				<VlcShares>
					<Plugins>
						<Helper>
							<Hoster>
								<file>Youtube.php</file>
							</Hoster>
							<file>Youtube.php</file>
						</Helper>
						<file>Youtube.php</file>
					</Plugins>
				</VlcShares>
			</X>
		</library>
		<public>
			<images>
				<icons>
					<hosters>
						<file>youtube.png</file>
					</hosters>
				</icons>
				<youtube>
					<file>logo.png</file>
					<file>no-thumbnail.png</file>
					<file>closedcaption.png</file>
					<uploads>
						<file>folder.png</file>
					</uploads>
					<icons>
						<file>activity.png</file>
						<file>account.png</file>
						<file>accounts.png</file>
						<file>uploaded.png</file>
						<file>library.png</file>
						<file>playlist.png</file>
						<file>favorites.png</file>
						<file>activities.png</file>
						<file>video.png</file>
						<file>subscriptions.png</file>
						<file>category.png</file>
						<file>subscription.png</file>
						<file>playlists.png</file>
					</icons>
				</youtube>
			</images>
			<css>
				<youtube>
					<file>style.css</file>
				</youtube>
			</css>
			<js>
				<youtube>
					<file>index.js</file>
					<file>category.js</file>
				</youtube>
			</js>
		</public>
		<languages>
			<file>X_VlcShares_Plugins_Youtube.en_GB.ini</file>
			<file>X_VlcShares_Plugins_Youtube.it_IT.ini</file>
			<file>X_VlcShares_Plugins_Youtube.es_ES.ini</file>
		</languages>
	</files>
	<database>
		<install>install.sql</install>
		<uninstall>uninstall.sql</uninstall> 
	</database>
</vs-manifest>
