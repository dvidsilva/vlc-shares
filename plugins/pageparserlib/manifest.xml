<?xml version="1.0" encoding="UTF-8"?>
<vs-manifest type="plugin">
	<metadata>
		<label>Lib_PageParser</label>
		<description>Add PageParser Api in 0.5.4 version</description>
		<version>0.1</version>
		<status>alpha2</status>
		<key>pageparserlib</key>
		<file>X/VlcShares/Plugins/PageParserLib.php</file>
		<class>X_VlcShares_Plugins_PageParserLib.php</class>
		<compatibility>
			<from>0.5.4</from>
			<to>0.5.5</to>
		</compatibility>
	</metadata>
	<files>
		<library>
			<X>
				<PageParser>
					<file>AuthDeposit.php</file>
					<file>AuthStrategy.php</file>
					<file>Loader.php</file>
					<file>Page.php</file>
					<file>Parser.php</file>
					<AuthDeposit>
						<file>Session.php</file>
						<file>Volatile.php</file>
					</AuthDeposit>
					<AuthStrategy>
						<file>Dummy.php</file>
					</AuthStrategy>
					<Loader>
						<file>Http.php</file>
						<file>HttpAuthRequired.php</file>
					</Loader>
					<Parser>
						<file>AggregateChain.php</file>
						<file>HosterLinks.php</file>
						<file>Json.php</file>
						<file>Preg.php</file>
						<file>RecursiveChain.php</file>
						<file>SimpleXml.php</file>
					</Parser>
				</PageParser>
				<VlcShares>
					<Plugins>
						<file>PageParserLib.php</file>
						<file>Utils.php</file>
					</Plugins>
				</VlcShares>
			</X>
		</library>
	</files>
	<database>
		<install>install.sql</install>
	</database>
</vs-manifest>
