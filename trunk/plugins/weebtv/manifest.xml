<?xml version="1.0" encoding="UTF-8"?>
<vs-manifest type="plugin">
	<metadata>
		<label>p_weebtv_plglabel</label>
		<description>p_weebtv_plgdesc</description>
		<version>0.1</version>
		<status>beta2</status>
		<key>weebtv</key>
		<file>X/VlcShares/Plugins/WeebTv.php</file>
		<class>X_VlcShares_Plugins_WeebTv</class>
		<compatibility>
			<from>0.5.5</from>
			<to>0.6</to>
		</compatibility>
	</metadata>
	<files>
		<bin>
			<rtmpdump-weebtv-linux>
				<file>ChangeLog</file>
				<file>COPYING</file>
				<file>README</file>
				<file>README2</file>
				<file>rtmpdump-weebtv</file>
				<file>rtmpgw-weebtv</file>
				<file>rtmpsrv-weebtv</file>
				<file>rtmpsuck-weebtv</file>
			</rtmpdump-weebtv-linux>
			<rtmpdump-weebtv-win>
				<file>ChangeLog</file>
				<file>COPYING</file>
				<file>README.txt</file>
				<file>README2.txt</file>
				<file>rtmpdump-weebtv.exe</file>
				<file>rtmpgw-weebtv.exe</file>
				<file>rtmpsrv-weebtv.exe</file>
				<file>rtmpsuck-weebtv.exe</file>
				<file>librtmp.dll</file>
				<file>zlib1.dll</file>
			</rtmpdump-weebtv-win>
		</bin>
		<library>
			<X>
				<Streamer>
					<Engine>
						<file>RtmpDumpWeebTv.php</file>
					</Engine>
				</Streamer>
				<VlcShares>
					<Plugins>
						<file>WeebTv.php</file>
					</Plugins>
				</VlcShares>
				<file>RtmpDumpWeebTv.php</file>
			</X>
		</library>
		<public>
			<images>
				<weebtv>
					<file>logo.png</file>
				</weebtv>
			</images>
		</public>
		<languages>
			<file>X_VlcShares_Plugins_WeebTv.en_GB.ini</file>
			<file>X_VlcShares_Plugins_WeebTv.it_IT.ini</file>
		</languages>
	</files>
	<database>
		<install>install.sql</install>
		<uninstall>uninstall.sql</uninstall>
	</database>
</vs-manifest>
